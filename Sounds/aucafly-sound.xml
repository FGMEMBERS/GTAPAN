<?xml version="1.0"?>
 
<PropertyList>
 <fx>


<!-- Current Engine Sound -->
    <engine>
      <name>engine #1 running</name>
      <mode>looped</mode>
      <path>Aircraft/AUCAFLYCORE/E1.wav</path>
<condition>		
  <greater-than-equals>		
   <property>velocities/groundspeed-kt</property>
   <value>20</value>
  </greater-than-equals>
 </condition>
  <volume>
        <property>engines/engine/mp-osi</property>
        <factor>0.00375</factor>
        <min>0.5</min>
        <max>2.0</max>
      </volume>
      <pitch>
        <property>engines/engine/rpm</property>
        <factor>0.0005</factor>
        <min>0.2</min>
        <max>2.0</max>
        <offset>0.15</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

<click>
      <name>brake-parking on</name>
      <path>Aircraft/AUCAFLYCORE/Park-Break-On.wav</path>
       <condition>
          <property>controls/gear/brake-parking</property>
    </condition>    
      <volume>
           <factor>2.0</factor>
      </volume>
       <reference-dist>100</reference-dist>
    <max-dist>1000</max-dist>      
    </click>

    <click>
      <name>brake-parking off</name>
      <path>Aircraft/AUCAFLYCORE/Park-Break-Off.wav</path>
       <condition>
         <not>
             <property>controls/gear/brake-parking</property>
          </not>
    </condition>    
      <volume>
           <factor>2.0</factor>
      </volume>
       <reference-dist>100</reference-dist>
    <max-dist>1000</max-dist>      
    </click>
<click>
      <name>indicator left</name>
	<mode>looped</mode>
      <path>Aircraft/AUCAFLYCORE/indicator.wav</path>
       <condition>
             <property>controls/lighting/indicator-left</property>
    </condition>    
      <volume>
           <factor>2.0</factor>
      </volume>
       <reference-dist>100</reference-dist>
    <max-dist>1000</max-dist>      
    </click>
<click>
      <name>indicator right</name>
	<mode>looped</mode>
      <path>Aircraft/AUCAFLYCORE/indicator.wav</path>
       <condition>
             <property>controls/lighting/indicator-right</property>
    </condition>    
      <volume>
           <factor>2.0</factor>
      </volume>
       <reference-dist>100</reference-dist>
    <max-dist>1000</max-dist>      
    </click>

 </fx>
</PropertyList>
